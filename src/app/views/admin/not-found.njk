{% extends "admin/layout-admin.njk" %}

{% block content %}

	<div class="container">
		<div class="card">
			<div class="error-lottie not-found"></div>

			<a href="/admin">Home Page</a>
		</div>
	</div>
{% endblock %}

{% block script %}
	<script src="/lottie.min.js"></script>

	<script>
		const element = document.querySelector('.error-lottie')

		lottie.loadAnimation({
			container: element, // the dom element that will contain the animation
			renderer: 'svg',
			loop: true,
			autoplay: true,
			path: '/24796-404-page.json' // the path to the animation json
		});
	</script>
{% endblock %}